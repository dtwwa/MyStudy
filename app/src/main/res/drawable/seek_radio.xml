<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- solid表示远的填充色 -->
    <solid android:color="#FE2D4A" />
    <!-- stroke则代表远的边框线 -->
    <stroke
        android:width="1dp"
        android:color="#FFFFFF" />
    <!-- size控制高宽 -->
    <size
        android:width="10dp"
        android:height="10dp" />
</shape>
